{
    "name": "vicoders/test-post-manage",
    "description": "learn pakage laravel",
    "autoload": {
        "psr-4": {
            "VCComponent\\Laravel\\TestPostManage\\": "src/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\TestPostManage\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "truongvuongquat"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.4"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    },
    "require": {
        "league/fractal": "^0.20.1"
    }
}
